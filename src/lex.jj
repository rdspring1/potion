
PARSER_BEGIN(Compiler)
import ast.*;
import java.util.List;
import java.util.ArrayList;
public class Compiler
{
	public static void main(String[] args)
	{
		Program p;
		try {
			p =  new Compiler(System.in).Program();

        } catch (Throwable e) {
